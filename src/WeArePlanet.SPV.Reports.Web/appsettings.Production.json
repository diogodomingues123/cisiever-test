{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": false,
          "SkipValidation": true
        }
      }
    }
  },
  "ConnectionStrings": {
    "Mongo": "mongodb://reports-db/?replSet=reports-rs"
  },
  "Auth": {
    "Domain": "https://planet-login-dev.weareplanet-dev.auth0app.com",
    "Audience": "tbd"
  },
  "Hangfire": {
    "DisplayStorageConnectionString": false
  },
  "Documentation": {
    "Swagger": {
      "Enabled": false
    }
  }
}
